#C
 input: 6
 ouput: 720

#include<stdio.h>

int main(void){
    int n;
    scanf("%d",&n);
    int a = fact(n);
    printf("%d",a);
    }
    
 int fact(int n){
     if(n==0){
         return 1;
     }
     else{
         return n*fact(n-1);
     }
 }
 
 #Python
 n = int(input())
 fact = 1
 for i in range(1,n+1):
       fact = fact*i
  print(fact)
